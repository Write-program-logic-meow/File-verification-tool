<Window x:Class="File_check.Setting_interface"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:File_check"
        mc:Ignorable="d"
        Title="设置" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <CheckBox x:Name="CheckBox1" Content="是否开机启动" HorizontalAlignment="Center" Margin="0,75,0,0" VerticalAlignment="Top" Checked="CheckBox1_Checked" Unchecked="CheckBox1_Unchecked"/>
        <CheckBox x:Name="CheckBox2" Content="夜间模式" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="CheckBox2_Checked" Unchecked="CheckBox2_Unchecked" Margin="0,127,0,0"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,147,0,0" TextWrapping="Wrap" Text="『夜间模式功能还未实现』" VerticalAlignment="Top"/>

    </Grid>
</Window>
